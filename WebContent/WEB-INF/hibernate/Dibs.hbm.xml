<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.luxoft.cantor.repository">

	<class name="Dibs" table="DIBS">
		<id name="dibsCode" />
		<property name="dibsName" />
		<set name="dibsRateList" table="dibsRateList" inverse="true"
			lazy="true" fetch="select">
			<key>
				<column name="dibsCode" not-null="true" />
			</key>
			<one-to-many class="com.luxoft.cantor.repository.DateAndRateOfDibs" />
		</set>
	</class>

</hibernate-mapping>